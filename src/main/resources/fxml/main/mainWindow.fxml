      fx:controller="orthae.com.github.medicalmanagementsystem.client.MainWindow"
      prefHeight="800.0" prefWidth="1200.0">
    <TabPane VBox.vgrow="ALWAYS">
        <Tab text="Dashboard" closable="false">
