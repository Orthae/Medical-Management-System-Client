        </Tab>
        <Tab text="Management" closable="false">
            <fx:include source="../management/managementWindow.fxml"/>
        </Tab>
        <Tab text="Business" closable="false">

        </Tab>
    </TabPane>
    <Separator/>
    <fx:include source="statusBar.fxml"/>
