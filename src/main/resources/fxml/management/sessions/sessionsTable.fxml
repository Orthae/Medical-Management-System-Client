<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="orthae.com.github.medicalmanagementsystem.client.management.sessions.SessionsTableController">
    <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="idColumn" text="Id"/>
            <TableColumn fx:id="activeColumn" text="Active"/>
            <TableColumn fx:id="usernameColumn" text="Username"/>
            <TableColumn fx:id="ipAddressColumn" text="IP Address"/>
            <TableColumn fx:id="creationDateColumn" text="Created"/>
            <TableColumn fx:id="expiryDateColumn" text="Expiring"/>
        </columns>
    </TableView>
</VBox>
